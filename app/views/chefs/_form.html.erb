<%= render 'shared/errors', obj: @chef %>

<div class="row">
  <div class="well col-md-6 col-md-offset-3">
    <%= form_for @chef do |f| %>
      <%= f.label :chefname %>
      <%= f.text_field :chefname %>
      
      <%= f.label :email %>
      <%= f.email_field :email %>
      
      <%= f.label :password %>
      <%= f.password_field :password %>
      
      <%= f.submit(@chef.new_record? ? 'Register' : 'Edit Profile', class: 'btn btn-success submit-btn') %>
    <% end %>
  </div>
</div>